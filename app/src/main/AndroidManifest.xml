<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alien.xenocorez.plarnpoung" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FoodListActivity"
            android:label="@string/title_activity_food_list"
            android:windowSoftInputMode="stateHidden" >
        </activity>
        <activity
            android:name=".foodInsertActivity"
            android:label="@string/title_activity_food_insert" >
        </activity>
        <activity
            android:name=".FoodCalculateActivity"
            android:label="@string/title_activity_food_calculate" >
        </activity>
    </application>

</manifest>
